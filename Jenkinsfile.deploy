pipeline {
  agent any
  stages{
      stage ('INIT') {
        steps {
          bat 'terraform init'
        }
      }
      stage ('VALIDATE') {
        steps {
          bat 'terraform validate'
        }
      }
      stage ('PLAN') {
        steps {
          bat 'terraform plan'
        }
      }
      stage ('APPLY') {
        steps {
          bat 'terraform apply --auto-approve'
        }
      }
  }
}
